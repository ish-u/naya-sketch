{
  "name": "naya-sketch",
  "version": "1.0.0",
  "description": "A Collborative Sketch Tool",
  "scripts": {
    "server": "npm run dev --prefix server",
    "client": "npm run dev --prefix client",
    "db": "docker-compose -f database/docker-compose.yml -p naya-sketch-db up --build",
    "dev": "concurrently \"npm run client\"  \"npm run db\" \"npm run server\""
